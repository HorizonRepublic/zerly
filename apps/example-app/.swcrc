{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true
    },
    "transform": {
      "decoratorMetadata": true
    },
    "loose": false,
    "externalHelpers": false,
    "keepClassNames": true,
    "preserveAllComments": true
  },
  "module": {
    "type": "commonjs",
    "strict": true,
    "noInterop": false
  },
  "sourceMaps": true
}
